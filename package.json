{
  "name": "adonis-rabbit",
  "description": "AdonisJS RabbitMQ provider",
  "version": "0.3.0",
  "author": "Jota JÃºnior <jjunior@protonmail.ch>",
  "contributors": [
    "Atakan Senturk <atkansenturk@gmail.com>"
  ],
  "main": "build/index.js",
  "type": "module",
  "license": "MIT",
  "repository": "https://github.com/jotaajunior/adonis-rabbit.git#readme",
  "prettier": "@adonisjs/prettier-config",
  "engines": {
    "node": ">=20.6.0"
  },
  "files": [
    "build"
  ],
  "exports": {
    ".": "./build/index.js",
    "./types": "./build/src/types/index.js",
    "./providers/rabbit_provider": "./build/providers/rabbit_provider.js",
    "./services/rabbit": "./build/services/rabbit.js"
  },
  "eslintConfig": {
    "extends": "@adonisjs/eslint-config/package"
  },
  "scripts": {
    "clean": "del-cli build",
    "copy:templates": "copyfiles \"stubs/**/*.stub\" build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "prebuild": "npm run lint && npm run clean",
    "build": "tsc",
    "postbuild": "npm run copy:templates",
    "release": "np",
    "version": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.7.0",
    "@adonisjs/core": "^6.14.1",
    "@adonisjs/eslint-config": "^1.3.0",
    "@adonisjs/prettier-config": "^1.3.0",
    "@adonisjs/tsconfig": "^1.3.0",
    "@swc/core": "^1.6.3",
    "@types/node": "^20.14.5",
    "c8": "^10.1.2",
    "copyfiles": "^2.4.1",
    "del-cli": "^5.1.0",
    "eslint": "^8.57.0",
    "np": "^10.0.6",
    "prettier": "^3.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "@adonisjs/core": "^6.14.1"
  },
  "dependencies": {
    "@types/amqplib": "^0.10.5",
    "amqplib": "^0.10.4",
    "fast-safe-stringify": "^2.1.1"
  },
  "np": {
    "message": "Releases v%s"
  }
}
